        <TextBlock x:Name="txtMBTI" HorizontalAlignment="Left" Margin="81,223,0,0" Grid.Row="1" TextWrapping="Wrap" Text="Tes Kepribadian MBTI: Kenali Dirimu!" VerticalAlignment="Top" FontSize="18" FontWeight="Bold" Height="80" Width="159" Foreground="#FF5E5E5E"/>
        <TextBlock x:Name="txtSepi" HorizontalAlignment="Left" Margin="690,222,0,0" Grid.Row="1" TextWrapping="Wrap" Text="Tes Tingkat Rasa Kesepian: Loneliness Scale" VerticalAlignment="Top" FontSize="18" FontWeight="Bold" Height="74" Width="180" Foreground="#FF5E5E5E"/>
