        <Label Content="Tes Psikologi" HorizontalAlignment="Left" Margin="30,193,0,0" VerticalAlignment="Top" FontSize="36" Grid.RowSpan="2" FontWeight="Bold" Foreground="#FF5E5E5E"/>
